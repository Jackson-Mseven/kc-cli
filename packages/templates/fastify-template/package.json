{
  "name": "{{name}}",
  "version": "0.1.0",
  "description": "a fastify template.",
  "author": "kincy <bill714@foxmail.com>",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "prepare": "husky",
    "commit": "git add . && cz",
    "cz-init": "pnpm dlx commitizen init cz-conventional-changelog --pnpm --save-dev --save-exact",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "cli",
    "fastify",
    "prisma",
    "postgres",
    "clickhouse",
    "swagger",
    "swagger-ui",
    "multer",
    "zod",
    "typescript",
    "nodejs",
    "pnpm"
  ],
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/postgres": "^6.0.2",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.10.1",
    "fastify": "^5.4.0",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "pg": "^8.16.2",
    "pino": "^8.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.1",
    "prisma": "^6.10.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.2.0"
  }
}
